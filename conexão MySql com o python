from sqlalchemy import create_engine
from sqlalchemy.ext.declarative import declarative_base
from sqlalchemy import Column, String, Integer
from sqlalchemy.orm import sessionmaker
import pymysql


engine = create_engine(
    "mysql+pymysql://root:vinicius123!@localhost:3306/teamtune")
Base = declarative_base()
Session = sessionmaker(bind=engine)
session = Session()


class Colaborador(Base):
    __tablename__ = 'colaborador'

    matricula = Column(Integer, primary_key=True)
    nome = Column(String, nullable=False)
    email = Column(String, nullable=False)
    senha = Column(String, nullable=False)


def __repr__(self):
    return f"Colaboradores (matricula={self.matricula},nome={self.nome},email={self.email},senha={self.senha})"

colaborador = session.query(Colaborador).first()
print(colaborador.matricula,colaborador.nome,colaborador.email,colaborador.senha)
